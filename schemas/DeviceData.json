{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://raw.githubusercontent.com/tryterra/openapi/refs/heads/v5-update/schemas/DeviceData.json",
    "type": "object",
    "properties": {
        "activation_timestamp": {
            "type": "string"
        },
        "data_provided": {
            "type": "array",
            "items": {
                "$ref": "https://raw.githubusercontent.com/tryterra/openapi/refs/heads/v5-update/schemas/DeviceDataType.json"
            }
        },
        "hardware_version": {
            "type": "string"
        },
        "last_upload_date": {
            "type": "string"
        },
        "manufacturer": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "other_devices": {
            "type": "array",
            "items": {
                "$ref": "https://raw.githubusercontent.com/tryterra/openapi/refs/heads/v5-update/schemas/OtherDeviceData.json"
            }
        },
        "sensor_state": {
            "type": "string"
        },
        "serial_number": {
            "type": "string"
        },
        "software_version": {
            "type": "string"
        }
